/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@guards/*": [
        "src/app/_guards/*"
      ],
      "@service/*": [
        "src/app/service/*"
      ],
      "@validators/*": [
        "src/app/validators/*"
      ],
      "@interface/*": [
        "src/app/interface/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@component/*": [
        "src/app/components/*"
      ],
      "@directive/*": [
        "src/app/directives/*"
      ],
      "@module/*": [
        "src/app/module/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "useDefineForClassFields": false
  }
}
